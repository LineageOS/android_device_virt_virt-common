# Bluetooth
persist.bluetooth.a2dp_aac.vbr_supported=true

# DHCP
vendor.recovery.dhcpclient.iface?=eth0

# Display
ro.surface_flinger.enable_frame_rate_override=false

# Input
qemu.hw.mainkeys=0

# Recovery
ro.recovery.batteryless=true

# VirtWifi
ro.boot.wifi_mac_prefix=5554
ro.vendor.virtwifi.port=eth0
